package Day5;

import java.util.ArrayList;

public class Looping {
public static void main(String[] args) {
	ArrayList al = new ArrayList();//10->11
	//newcap = currentcap * 1.5 + 1
	
	al.add(10);
	al.add(70);
	al.add("Hello");
	al.add(3.5);
	System.out.println(al);

	for(Object a : al) {
		System.out.println(a);
	}
}
}
